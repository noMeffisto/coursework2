[run]
source = main
omit =
    */migrations/*
    */tests/*
    */admin.py
    */apps.py
    */urls.py
    */wsgi.py
    */asgi.py
    manage.py
    */settings.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    if settings.DEBUG
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError 